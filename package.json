{
  "name": "sc4",
  "version": "0.0.16",
  "description": "A command line utility for automating SimCity 4 modding tasks & modifying savegames",
  "main": "lib/index.js",
  "scripts": {
    "install": "node-gyp-build",
    "test": "mocha",
    "prebuild": "prebuildify --napi --strip & prebuildify --napi --strip --arch ia32",
    "build:cpp": "node-gyp rebuild",
    "electron": "electron lib/gui/app.js"
  },
  "bin": {
    "sc4": "./bin/cli.js"
  },
  "keywords": [
    "SimCity 4",
    "modding"
  ],
  "author": "Sebastiaan Marynissen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sebamarynissen/sc4.git"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "electron": "^5.0.6",
    "mocha": "^6.2.2",
    "node-gyp": "^4.0.0",
    "prebuildify": "^3.0.4",
    "string-argv": "^0.3.1",
    "three": "^0.106.2"
  },
  "dependencies": {
    "binary-search": "^1.3.6",
    "chalk": "^2.4.2",
    "commander": "^2.20.3",
    "ini": "^1.3.5",
    "inquirer": "^6.5.2",
    "node-addon-api": "^1.7.1",
    "node-gyp-build": "^4.2.1",
    "ora": "^3.4.0",
    "p-queue": "^6.3.0",
    "tar": "^4.4.13",
    "util-deprecate": "^1.0.2",
    "xml2js": "^0.4.23"
  }
